[ req ]
default_bits           = 2048
distinguished_name     = req_distinguished_name
req_extensions         = req_ext

[ req_distinguished_name ]
countryName            = MY
stateOrProvinceName    = PG
localityName           = GT
organizationName       = UT
commonName             = www.localhost:1337

[ req_ext ]
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
IP.1   = 127.0.0.1

